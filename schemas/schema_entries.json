{
  "entry_layer": {
    "sets": {
      "pullback_trend": {
        "context": { "min_buy_prob": 0.60, "min_adx": 0, "volatility_gate_atr_pct_min": 0.0 },
        "filters": { "volume_percentile": 0.60, "volume_multiplier_vs_mean20": 1.3 },
        "signals": {
          "touch": { "type": "or", "levels": ["EMA20","VWAP_sigma_1","BB_lower"] },
          "rsi_cross": { "length": 7, "level": 50, "direction": "up" },
          "candle_body_above_median": true
        },
        "risk": { "sl_atr_mult": 1.2, "tp_atr_mult": 1.8, "trail_after_profit_atr": 1.0, "trail_step_atr": 1.0 }
      },
      "breakout": {
        "context": { "min_buy_prob": 0.55, "min_adx": 20 },
        "signals": {
          "close_above_high_n": 20,
          "macd_hist_increasing": true,
          "squeeze": { "bb_length": 20, "bb_stddev": 2.0, "keltner_length": 20, "keltner_atr_mult": 1.5, "condition": "bb_inside_kc_then_expand" }
        },
        "risk": { "sl_method": "below_range_n_low", "tp_method": "range_height_or_atr", "tp_atr_mult": 1.5 }
      }
    },
    "common": {
      "vwap": { "session": "daily", "bands_sigma": [1, 2] },
      "bbands": { "length": 20, "stddev": 2.0 },
      "keltner": { "length": 20, "atr_mult": 1.5 },
      "fractals": { "lookback": 5 },
      "timeout_bars": 25
    }
  },
  "execution": {
    "prefer_maker": true,
    "post_only": true,
    "allow_market_on_breakout": true,
    "order_timeout_sec": 20,
    "cancel_on_timeout": true
  }
}